<div class="Header">
	<div class="Header-logo">
		<a href="/SIA">
			<img src="/SIA/juridico/Dev/Assets/images/Logotipo.png">
		</a>
	</div>
	<div class="Header-cuenta">
		<h4>
			<i class="fas fa-archive"></i>
			{{ home.header.cuentaPublica }}
		</h4>
	</div>
	<div class="Header-module">
		<p>
			<i class="fas fa-desktop"></i>
			{{ home.header.modulo }}
		</p>
	</div>
	<div class="Header-notifications">
		<a href="#!">
			<i class="far fa-envelope"></i>
			Tienes
			<strong>{{ home.header.notificaciones }}</strong>
			Notificaciones
		</a>
	</div>
	<div class="Header-user">
		<p>
			<i class="fas fa-user"></i>
			{{ home.header.usuario }}
			<i class="fas fa-caret-down"></i>
		</p>
		<div class="Header-user-submenu">
			<a href="#!">
				<i class="fas fa-user"></i>
				Perfil
			</a>
			<a href="#!">
				<i class="fas fa-sign-out-alt"></i>
				Salir
			</a>
		</div>
	</div>
</div>